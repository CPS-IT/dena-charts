{
	"name": "cpsit/dena-charts",
	"type": "typo3-cms-extension",
	"description": "Charts allows two display data sets as charts. This is a TYPO3 extension for the DENA project",
	"keywords": [
		"TYPO3",
		"extension",
		"import",
		"export"
	],
	"license": [
		"GPL-2.0+"
	],
	"authors": [
		{
			"name": "Dirk Wenzel",
			"role": "Developer"
		}
	],
	"require": {
		"typo3/cms-core": "^10.4"
	},
	"replace": {
		"dena_charts": "self.version"
	},
	"require-dev": {
		"mikey179/vfsStream": "~1.6.0",
		"nimut/testing-framework": "^1.1",
		"phpunit/phpunit": "^4.8 || ^5.6"
	},
	"autoload": {
		"psr-4": {
			"CPSIT\\DenaCharts\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CPSIT\\DenaCharts\\Tests\\": "Tests"
		}
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"scripts": {
		"post-autoload-dump": [
			"mkdir -p .Build/Web/typo3conf/ext/",
			"mkdir -p .Build/log/coverage/",
			"[ -L .Build/Web/typo3conf/ext/dena_charts || ln -snvf ../../../../. .Build/Web/typo3conf/ext/dena_charts"
		]
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}

